{
  "@header": {
    "version": "0.0.1",
    "aasVersion": "3.0RC01",
    "@variables": {
    },
    "@namespaces": {
      "sparql": "http://www.w3.org/2005/sparql-results#",
      "semanticId": "https://w3id.org/catenax/ontology/aas#"
    },
    "@definitions": {
    }
  },
  "aasEnvironmentMapping": {
    "assetAdministrationShells": [
      {
        "@foreach": {
          "@xpath": "//sparql:result"
        },
        "assetInformation": {
          "assetKind": "Instance",
          "globalAssetId": {
            "keys": [
              {
                "@bind": {
                  "value": {
                    "@xpath": "./sparql:binding[@name='id']/sparql:uri"
                  }
                },
                "idType": "Iri",
                "type": "AssetAdministrationShell"
              }
            ]
          }
        },
        "@bind": {
          "idShort": {
            "@xpath": "./sparql:binding[@name='id']/sparql:uri"
          }
        },
        "identification": {
          "@bind": {
            "id": {
              "@generate_uuid": {
                "@xpath": "./sparql:binding[@name='id']/sparql:uri"
              }
            }
          },
          "idType": "Iri"
        },
        "submodels": [
          {
            "@foreach": {
              "@xpath": "./sparql:binding[@name!='id']"
            },
            "keys": [
              {
                "idType": "Iri",
                "type": "Submodel",
                "@bind": {
                  "value": {
                    "@xpath": "./sparql:binding[@name='id']/sparql:uri"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}