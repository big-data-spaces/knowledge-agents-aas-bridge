{
  "@header": {
    "version": "0.0.1",
    "aasVersion": "3.0RC01",
    "@variables": {
    },
    "@namespaces": {
      "sparql": "http://www.w3.org/2005/sparql-results#",
      "semanticId": "https://w3id.org/catenax/ontology/aas#"
    },
    "@definitions": {
    }
  },
  "aasEnvironmentMapping": {
    "assetAdministrationShells": [
      {
        "@foreach": {
          "@xpath": "//sparql:result"
        },
        "@bind": {
          "idShort": {
            "@xpath": "./sparql:binding[@name='id']/sparql:uri"
          }
        },
        "identification": {
            "@bind": {
              "id": {
                "@xpath": "./sparql:binding[@name='id']/sparql:uri"
              }
            },
            "idType": "Iri"
        },
        "description": [{
          "language": "en",
          "@bind": {
            "text": {
              "@xpath": "./sparql:binding[@name='name']/sparql:literal"
            }
          }
        }],
        "assetInformation": {
          "assetKind": "Instance",
          "globalAssetId": {
            "keys": [
              {
                "@bind": {
                  "value": {
                    "@xpath": "./sparql:binding[@name='id']/sparql:uri"
                  }
                },
                "idType": "Iri",
                "type": "AssetAdministrationShell"
              }
            ]
          }
        },
        "submodels": [
          {
            "@foreach": {
              "@xpath": "./sparql:binding[@name != 'id'and @name != 'name']"
            },
            "keys": [
              {
                "idType": "Iri",
                "type": "Submodel",
                "@bind": {
                  "value": {
                    "@concatenate": [
                      {"@xpath":  "./sparql:uri"},
                      "/",
                      {"@xpath":  "../sparql:binding[@name = 'id']/sparql:uri"}
                    ]
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}